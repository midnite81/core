{
    "preset": "laravel",
    "rules": {
        "not_operator_with_space": false,
        "not_operator_with_successor_space": false,
        "concat_space": {
            "spacing": "one"
        },
        "types_spaces": {
            "space": "none",
            "space_multiple_catch": "none"
        },
        "phpdoc_align": {
            "align": "left"
        },
        "no_superfluous_phpdoc_tags": {
            "allow_mixed": true,
            "allow_unused_params": true,
            "remove_inheritdoc": false
        }
    }
}
